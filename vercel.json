{
  "version": 2,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.6.0"
    }
  },
  "routes": [
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1"
    },
    {
      "src": "/css/(.*)",
      "dest": "/public/css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "/public/js/$1"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/api/index.php"
    },
    {
      "src": "/auth/(.*)",
      "dest": "/api/index.php"
    },
    {
      "src": "/emails/(.*)",
      "dest": "/api/index.php"
    },
    {
      "src": "/layout/(.*)",
      "dest": "/api/index.php"
    },
    {
      "src": "/layout-admin/(.*)",
      "dest": "/api/index.php"
    },
    {
      "src": "/user/(.*)",
      "dest": "/api/index.php"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/index.php"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }

  ]
}